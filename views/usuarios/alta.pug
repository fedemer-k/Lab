html 
    head
        link(rel="stylesheet"
         href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" 
         integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" 
         crossorigin="anonymous")
    body
        .container
            br
            form.form(action="/usuarios" method="post" enctype="multipart/form-data")
                .form-group
                    label Nombre 
                    input.form.form-control(type = "text" name = "nombre")
                .form-group 
                    label Especialidad 
                    select.form.form-control(name="especialidad")
                        each especialidad in especialidades 
                            option(value=especialidad.id) #{especialidad.descripcion} 
                .form-group 
                    label edad 
                    input.form.form-control(type="number" name="edad")
                .form-group 
                    label foto 
                    input.form.form-control(type="file" name="imagen")
                .form-group 
                    input.form-control.btn.btn-primary(type="submit" value="alta")